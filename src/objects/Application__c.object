    <validationRules>
        <fullName>COE_request_should_be_sent_after_complet</fullName>
        <active>true</active>
        <description>COE request should be sent after completing the offer conditions.
Validation: If (Offer Type == conditional  &amp;&amp; IsConditionMet = true || Offer Type == Unconditional) then allow update to Status = Payment Made
Consider: DoPaymentAfterVisa field == true</description>
        <errorConditionFormula>AND (
     ISPICKVAL(Status__c,&quot;Payment Made&quot;),
     Preferred_University__r.DoPaymentAfterVisa__c == True,
        NOT(
           OR(
	      AND(
                  ISPICKVAL(Offer_Type__c,&quot;Conditional&quot;)
                  ,Is_Conditions_Met__c = True
              ),
	      AND(
                 ISPICKVAL(Offer_Type__c,&quot;Unconditional&quot;)
                ,Is_Conditions_Met__c = False
	      )
	   )    
       )
)</errorConditionFormula>
        <errorMessage>Status Can&apos;t be Updated to &apos;Payment Made&apos; until the Conditions are Met.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offer_condition</fullName>
        <active>true</active>
        <description>Based on Offer Type Offer Conditions should be displayed</description>
        <errorConditionFormula>AND(
    ISPICKVAL( Offer_Type__c ,&apos;Conditional&apos;),
    ISBLANK( Offer_Conditions__c )
)</errorConditionFormula>
        <errorDisplayField>Offer_Conditions__c</errorDisplayField>
        <errorMessage>Offer Conditions are not Visible</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_End_Date_GreatProgram_Start_Date</fullName>
        <active>true</active>
        <description>Program End Date Should be Greater than Program Start Date</description>
        <errorConditionFormula>Program_Start_date__c &gt;  Program_end_date__c</errorConditionFormula>
        <errorDisplayField>Program_end_date__c</errorDisplayField>
        <errorMessage>Program End Date Should be Greater than Program Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_fields_for_Offer_Recieved</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
ISPICKVAL( Status__c ,&quot;Offer Received&quot;), 
ISPICKVAL( Status__c ,&quot;Payment Made&quot;), 
ISPICKVAL( Status__c ,&quot;COE/CAS Received&quot;), 
ISPICKVAL( Status__c ,&quot;COE/CAS Rejected&quot;), 
ISPICKVAL( Status__c ,&quot;Visa Applied&quot;), 
ISPICKVAL( Status__c ,&quot;Visa Granted&quot;), 
ISPICKVAL( Status__c ,&quot;Visa Rejected&quot;), 
ISPICKVAL( Status__c ,&quot;Visa Withdraw&quot;) 
), 
OR( 
ISBLANK( Student_ID__c ), 
ISBLANK( Program_Start_date__c ), 
ISBLANK( Program_end_date__c ), 
ISBLANK( Full_Course_fees_Annual__c ), 
ISBLANK( Course_Name__c), 
ISBLANK(TEXT( Offer_Type__c )) 
) 
)</errorConditionFormula>
        <errorMessage>Please check the Required Fields Filled or not [ Student Id,Program Start Date,Program End Date,Full Course Fees,Course Name,Offer Type ]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Previous_Offer_on_Offer_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND (IsPickVal( Next_offer__c  , &quot;Offer 01&quot;) ,  Previous_Offer__c !=null)</errorConditionFormula>
        <errorDisplayField>Previous_Offer__c</errorDisplayField>
        <errorMessage>Offer 1 cannot have a previous Offer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Previous_Offer_on_Package_offer</fullName>
        <active>true</active>
        <errorConditionFormula>AND (IsPickVal(Package_Offer__c , &quot;No&quot;) ,  Previous_Offer__c !=null)</errorConditionFormula>
        <errorDisplayField>Previous_Offer__c</errorDisplayField>
        <errorMessage>Non-Package offer cannot have a previous Offer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>visa_granted_Applied_AcceptedDate_Manda</fullName>
        <active>true</active>
        <description>Accepted Date is Mandatory</description>
        <errorConditionFormula>AND( 
CASE(
        Status__c ,
        &quot;Visa Granted&quot;, 1,
        &quot;Visa Applied&quot;, 1,
        0
    ) = 1,
OR(
ISBLANK( Accepted_date__c)
))</errorConditionFormula>
        <errorDisplayField>Accepted_date__c</errorDisplayField>
        <errorMessage>Required Field is Missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>visa_granted_Applied_InitialFeeMandatory</fullName>
