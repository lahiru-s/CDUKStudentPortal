    <validationRules>
        <fullName>Invoice_is_University_False</fullName>
        <active>true</active>
        <description>Invoice Item Type (+/-) is Additions and Deductions then Scholarship,Fee,Rate Should be Empty.</description>
        <errorConditionFormula>AND(
    Invoice_Header__r.Invoice_for_University__c = FALSE,
    CASE(
        Invoice_Item_Type__c,
        &quot;Deduction&quot;, 1,
        &quot;Addition&quot;, 1,
        0
    ) = 1,
    OR(
        AND(
            NOT(ISBLANK(Fee__c))
        ),
        AND(
            NOT(ISBLANK(Rate__c))
        ),
        AND(
            NOT(ISBLANK(Scholarship__c)),
            Scholarship__c &lt;&gt; 0
        )
    )
)</errorConditionFormula>
        <errorDisplayField>Invoice_Item_Type__c</errorDisplayField>
        <errorMessage>Scholarship , Fee , Rate Should be Empty if Invoice for University is False</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_is_University_True</fullName>
        <active>true</active>
        <description>Invoice Item Type (+/-) is Deductions then Scholarship,Fee,Rate Should be Empty.</description>
        <errorConditionFormula>AND(
    Invoice_Header__r.Invoice_for_University__c = TRUE,
    ISPICKVAL(Invoice_Item_Type__c, &quot;Deduction&quot;),
    OR(
        AND(
            NOT(ISBLANK(Fee__c))
        ),
        AND(
            NOT(ISBLANK(Rate__c))
        ),
        AND(
            NOT(ISBLANK(Scholarship__c)),
            Scholarship__c &lt;&gt; 0
        )
    )
)</errorConditionFormula>
        <errorDisplayField>Invoice_Item_Type__c</errorDisplayField>
        <errorMessage>Scholarship,Fee and Rate should be Empty if it is Deduction</errorMessage>
    </validationRules>
