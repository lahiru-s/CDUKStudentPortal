<apex:page standardController="Lead__c"  showHeader="false" standardStylesheets="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0">
    <apex:messages />
    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
        <head>
            <meta charset="utf-8" />
            <meta http-equiv="x-ua-compatible" content="ie=edge" />
            <title>Lead Convert</title>
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <!-- Import the Design System style sheet -->
            <apex:slds />
        </head>
        
        <body>
            <!-- REQUIRED SLDS WRAPPER -->
            <div class="slds-scope">
                <!-- PAGE HEADER -->
                
                <!-- / PAGE HEADER -->
                
                <!-- PRIMARY CONTENT WRAPPER -->
                <div class="slds-p-horizontal_medium">
                    <div class="slds-grid slds-wrap">
                        <div class="slds-col slds-size_1-of-1 slds-large-size_2-of-12">
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-large-size_8-of-12">
                            <div class="slds-m-bottom_medium" style="padding: 0.25rem; background: #0066cc;">
                                <div class="slds-text-heading_medium slds-text-color_inverse slds-text-align_center">Lead convert</div>
                            </div>
                            <apex:form > 
                                <div class="slds-form slds-form_stacked">
                                    <div class="slds-form-element__row">
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Name :</label>
                                            <apex:inputField value="{!Lead__c.Name}" id="l1" styleClass="slds-input"  html-placeholder="Name"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">LastName :</label>
                                            <apex:inputField value="{!Lead__c.Last_Name__c}" id="l2" styleClass="slds-input "  html-placeholder="LastName"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">MiddleName:</label>
                                            <apex:inputField value="{!Lead__c.Middle_Names__c}" id="l3" styleClass="slds-input"  html-placeholder="MiddleName"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Mobile :</label>
                                            <apex:inputField value="{!Lead__c.Mobile__c}" id="l4" styleClass="slds-input"  html-placeholder="Mobile"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Email :</label>
                                            <apex:inputField value="{!Lead__c.Email__c}" id="l5" styleClass="slds-input"  html-placeholder="Email"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">DateOfBirth :</label>
                                            <apex:inputField value="{!Lead__c.DOB__c}" id="l6" styleClass="slds-input"  html-placeholder="DateOfBirth"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">City :</label>
                                            <apex:inputField value="{!Lead__c.City__c}" id="l7" styleClass="slds-input"  html-placeholder="City"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">PassportNumber :</label>
                                            <apex:inputField value="{!Lead__c.Passport_Number__c}" id="l8" styleClass="slds-input"  html-placeholder="PassportNumber"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Gender :</label>
                                            <apex:inputField value="{!Lead__c.Gender__c}" id="l9" styleClass="slds-input"  html-placeholder="Gender"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Country Intrested In :</label>
                                            <apex:inputField value="{!Lead__c.Country_Interested_In__c}"  id="l10" styleClass=""  html-placeholder="Country Intrested In"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Intrested Field in Study :</label>
                                            <apex:inputField value="{!Lead__c.Interested_Field_of_Study__c }"  id="unit" styleClass="slds-input"  html-placeholder="intrested field in study"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Current Qualification :</label>
                                            <apex:inputField value="{!Lead__c.Current_Qualification__c}"   id="l11" styleClass=""  html-placeholder="Current Qualification"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Expected Year to Begin:</label>
                                            <apex:inputField value="{!Lead__c.Expected_Year_to_Begin__c}"   id="l12" styleClass="slds-input"  html-placeholder="Expected year to Begin"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Address Line:</label>
                                            <apex:inputField value="{!Lead__c.Address_Line_1__c}" id="l13" styleClass="slds-input"  html-placeholder="Address Line"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">City:</label>
                                            <apex:inputField value="{!Lead__c.City_p__c}" id="l14" styleClass="slds-input"  html-placeholder="City"/>
                                        </div>
                                        <div class="slds-form-element slds-form-element__control slds-size--2-of-2">
                                            <label class="slds-form-element__label" for="input-05">Postal/Zip code:</label>
                                            <apex:inputField value="{!Lead__c.Postal_Zip_Code_p__c}" id="l15" styleClass="slds-input"  html-placeholder="Postal/Zip code"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-form-element" style="center;margin-top: 1em;">
                                    <!-- <apex:commandButton value="Convert" styleClass="slds-button slds-button--brand " action="{!convertLead}" onclick="validate()"/> -->
                                </div>
                            </apex:form>
                            <div class="slds-col slds-size_1-of-1 slds-large-size_1-of-12"> 
                            </div>
                            
                        </div>
                    </div>
                    <!-- / PRIMARY CONTENT WRAPPER -->
                    <!-- FOOTER -->
                    <!-- / FOOTER -->
                </div>
                <!-- / REQUIRED SLDS WRAPPER -->
                <!-- JAVASCRIPT -->
                <!-- / JAVASCRIPT -->
            </div>
        </body>
    </html>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css"/>
    <script src="//code.jquery.com/jquery-1.9.1.js"></script>
    <script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
    
    <script>
    function validate(){ 
        var j$ = jQuery.noConflict();
        var unit = j$("input[id$='Unit']").val();  
        if (unit == '') {  
            alert("Should provide web lead value");
        }
        </script>
</apex:page>